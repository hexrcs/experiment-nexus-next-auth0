{
  "name": "fullstack-graphql-next-nexus-prisma-auth0",
  "version": "0.1.0",
  "description": "Fullstack Graphql with Next.js, Nexus, Prisma, Auth0",
  "scripts": {
    "db:start": "docker-compose up -d db",
    "db:stop": "docker-compose stop db",
    "db:migrate": "npx -q prisma migrate save --experimental && npx -q prisma migrate up --experimental",
    "nexus:reflection": "nexus dev --reflection --entrypoint pages/api/graphql.ts",
    "nexus:check": "nexus build --entrypoint pages/api/graphql.ts && nexus build --entrypoint pages/api/playground.ts",
    "test": "nexus build",
    "dev": "next dev",
    "start": "next start",
    "build": "nexus build && next build",
    "deploy:preview": "now",
    "deploy:prod": "now --prod",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "build-storybook": "build-storybook",
    "db:clean": "/bin/rm -rf temp && npx -q prisma migrate up --experimental",
    "db:init": "npx -q prisma migrate up --experimental"
  },
  "keywords": [],
  "author": "Xiaoru Li",
  "dependencies": {
    "@auth0/nextjs-auth0": "^0.15.0",
    "@chakra-ui/core": "^0.8.0",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "emotion-theming": "^10.0.27",
    "graphql-tag": "^2.10.3",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^11.1.0",
    "next-urql": "^1.0.1",
    "nexus": "^0.24.2",
    "nexus-plugin-prisma": "^0.10.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-is": "^16.13.1",
    "urql": "^1.9.8"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@graphql-codegen/cli": "^1.15.4",
    "@graphql-codegen/typescript": "^1.15.4",
    "@graphql-codegen/typescript-operations": "^1.15.4",
    "@graphql-codegen/typescript-urql": "^1.15.4",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@types/node": "^12.12.47",
    "@types/react": "^16.9.38",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
